<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.order.mapper.CustomerMapper">

    <!--根据id查询Emp实体-->
    <update id="updateCustomerAge">
        update customer set age = #{age} where 1=1
        <if test="id!=null">
           and id = #{id}
        </if>
    </update>



</mapper>